{
  "name": "ts-hdb",
  "version": "0.1.7",
  "description": "hdb driver with typescript",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.39",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitlint": "^16.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "husky": "~4.3.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@newdash/newdash": "^5.21.2",
    "hdb": "^0.19.3"
  },
  "keywords": [
    "SAP",
    "HANA",
    "Database"
  ],
  "license": "Apache-2.0"
}
