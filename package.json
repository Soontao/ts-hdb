{
  "name": "ts-hdb",
  "version": "0.1.4",
  "description": "hdb driver with typescript",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "prettier": "prettier --check --write src",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^14.1.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "commitlint": "^14.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "~4.3.8",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@newdash/newdash": "^5.20.0",
    "debug": "^4.3.2",
    "hdb": "^0.19.0"
  },
  "keywords": [
    "SAP",
    "HANA",
    "Database"
  ],
  "license": "Apache-2.0"
}
